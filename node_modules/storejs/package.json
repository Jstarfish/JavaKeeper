{
  "name": "storejs",
  "version": "1.1.0",
  "description": "Local storage localstorage package provides a simple API",
  "homepage": "https://github.com/jaywcjlove/store.js",
  "main": "dist/store.cjs.js",
  "module": "dist/store.esm.js",
  "browser": "dist/store.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/store.js.git"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "node scripts/build.js",
    "watch": "node scripts/watch.js",
    "test": "jest --coverage && tsc -p ./types",
    "test:watch": "jest --coverage --watch"
  },
  "keywords": [
    "storejs",
    "store",
    "storage",
    "local",
    "localstorage"
  ],
  "files": [
    "dist/*.js",
    "types/index.d.ts"
  ],
  "author": "kenny wang <wowohoo@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaywcjlove/store.js/issues"
  },
  "jset": {
    "bail": true,
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-async-to-generator": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@type-challenges/utils": "^0.1.1",
    "bannerjs": "^1.0.7",
    "colors-cli": "^1.0.26",
    "jest": "^24.9.0",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "typescript": "^4.1.3",
    "zlib": "^1.0.5"
  },
  "dependencies": {}
}
