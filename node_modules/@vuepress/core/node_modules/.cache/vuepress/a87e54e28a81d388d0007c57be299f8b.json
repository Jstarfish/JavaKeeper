FS,QAGDC,UAHC,GAGc,KAAKF,YAHnB,CAGDE,UAHC;;AAIT,QAAIA,UAAU,KAAK,KAAnB,EAA0B;AACxB;AACA,UAAIH,SAAS,KAAK,MAAlB,EAA0B;AACxBI,QAAAA,MAAM,CAACC,UAAP,CAAkB,8BAAlB,EAAkDC,WAAlD,CAA8D,YAAM;AAClEb,UAAAA,SAAS,CAACO,SAAD,CAAT;AACD,SAFD;AAGAI,QAAAA,MAAM,CAACC,UAAP,CAAkB,+BAAlB,EAAmDC,WAAnD,CAA+D,YAAM;AACnEb,UAAAA,SAAS,CAACO,SAAD,CAAT;AACD,SAFD;AAGD;;AACDP,MAAAA,SAAS,CAACO,SAAD,CAAT;AACD;AACF,GApCY;AAsCbO,EAAAA,OAAO,EAAE;AACPC,IAAAA,QADO,sBACK;AACV,WAAKV,QAAL,GAAgB,KAAhB;AACD;AAHM;AAtCI,CAAf","sourcesContent":["//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n\nimport { RecoIcon, ModuleTransition } from '@vuepress-reco/core/lib/components'\nimport ClickOutside from 'vue-click-outside'\nimport ModePicker from './ModePicker'\nimport applyMode from './applyMode'\n\nexport default {\n  name: 'UserSettings',\n\n  directives: {\n    'click-outside': ClickOutside\n  },\n\n  components: {\n    ModePicker,\n    RecoIcon,\n    ModuleTransition\n  },\n\n  data () {\n    return {\n      showMenu: false\n    }\n  },\n\n  // 为了在保证 modePicker 在 SSR 中正确开关，并实现管理，Mode 组件将负责 modePicker 关闭的情况\n  mounted () {\n    // modePicker 关闭时默认使用主题设置的模式\n    const themeMode = this.$themeConfig.mode || 'auto'\n    const { modePicker } = this.$themeConfig\n    if (modePicker === false) {\n      // 为 'auto' 模式设置监听器\n      if (themeMode === 'auto') {\n        window.matchMedia('(prefers-color-scheme: dark)').addListener(() => {\n          applyMode(themeMode)\n        })\n        window.matchMedia('(prefers-color-scheme: light)').addListener(() => {\n          applyMode(themeMode)\n        })\n      }\n      applyMode(themeMode)\n    }\n  },\n\n  methods: {\n    hideMenu () {\n      this.showMenu = false\n    }\n  }\n}\n"]}]}